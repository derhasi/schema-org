<?php

namespace Spatie\SchemaOrg;

/**
 * Factory class for all Schema.org types.
 */
class Schema
{
{% for type in types %}
    /**
     * Create {{ type.name }}.
     *
     * return \Spatie\SchemaOrg\{{ type.name }}
     */
    public static function {{ type.name | lcfirst }}()
    {
        return new {{ type.name }}();
    }

{% endfor %}
}
